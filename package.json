{
  "name": "rescript-next",
  "description": "ReScript bindings for Next.js",
  "version": "10.0.0-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@rescript/react": "^0.10.0",
    "next": "^10.0.0"
  },
  "repository": "https://github.com/MoOx/rescript-next",
  "funding": "https://github.com/MoOx/rescript-next?sponsor=1",
  "license": "MIT",
  "keywords": [
    "rescript",
    "react",
    "next"
  ],
  "files": [
    "*.md",
    "bsconfig.json",
    "src/**/*.res",
    "src/**/*.resi",
    "src/**/*.js",
    "!src/**/*.bs.js"
  ],
  "scripts": {
    "res:start": "bsb -make-world -w",
    "res:build": "bsb -make-world",
    "res:clean-build": "bsb -clean-world -make-world",
    "start": "yarn res:start",
    "build": "yarn res:build",
    "test": "yarn res:clean-build",
    "release": "npmpub"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.0",
    "bs-platform": "^9.0.0",
    "npmpub": "^5.0.0",
    "prettier": "^2.0.0"
  }
}

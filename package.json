{
  "name": "rescript-next",
  "description": "ReScript bindings for Next.js",
  "version": "12.0.1",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@rescript/react": ">=0.10.3",
    "next": ">=12.0.0"
  },
  "repository": "https://github.com/MoOx/rescript-next",
  "funding": "https://github.com/MoOx/rescript-next?sponsor=1",
  "license": "MIT",
  "keywords": [
    "rescript",
    "react",
    "next"
  ],
  "files": [
    "*.md",
    "bsconfig.json",
    "src/**/*.res",
    "src/**/*.resi",
    "src/**/*.js",
    "!src/**/*.bs.js"
  ],
  "scripts": {
    "re:start": "rescript build -w",
    "re:build": "rescript",
    "re:clean-build": "rescript clean && rescript",
    "start": "npm run re:start",
    "build": "npm run re:build",
    "test": "npm run re:clean-build",
    "release": "npmpub"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.3",
    "npmpub": "^5.0.0",
    "prettier": "^2.0.0",
    "rescript": "^9.1.4"
  }
}
